
public class Product 
{

	void addProduct()
	{
		System.out.println("This is addProduct");
	}
	void addCategory()
	{
		System.out.println("This is addCategory");
	}
	
	
}

class Demo
{
	public static void main(String[] args) 
	{
		
       Product p1=new Product();
       p1.addProduct();
       p1.addCategory();
	}

}
